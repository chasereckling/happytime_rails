<%= render "layouts/errors", :object => @bar %>

<%= form_for @bar do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label "Happy time start" %>
<%= f.text_field :happyHourStart %>
<br>
<%= f.label "Happy time end" %>
<%= f.text_field :happyHourEnd %>
<br>
<%= f.label :address %>
<%= f.text_field :address %>
<br>
<%= f.label :ontap %>
<%= f.text_field :ontap %>
<br>
<%= f.submit %>
<% end %>
